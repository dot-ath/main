---
import type { RouteProps } from '@explorers-club/schema';
import { initAccessToken } from '../auth';
import Layout from '../layouts/Layout.astro';

const routeProps = { name: 'Login' } satisfies RouteProps;
const { accessToken, connectionId } = initAccessToken(
  Astro.cookies,
  routeProps,
  Astro.url.href
);
---

<Layout
  title="Explorers Game"
  routeProps={{ name: 'Login' }}
  accessToken={accessToken}
  connectionId={connectionId}
/>
